{
    "name": "",
    "version": "2.0",
    "description": "",
    "manifest_version": 2,
    "author": "Haniyeh Asemi",
    "icons": {
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
